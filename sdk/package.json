{
  "name": "fee-sdk",
  "version": "1.1.0-beta.1",
  "description": "init",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib/ --verbose --copy-files && node script/build.js",
    "analyze": "babel src -d lib/ --verbose --copy-files && cross-env Webpack_Analyze_Report=true node script/build.js",
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "test": "jest --coverage && open ./coverage/lcov-report/index.html"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix --ext .js",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [],
  "author": "lianjia-fe",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.7.7",
    "js-cookie": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "commitizen": "^4.0.3",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "ora": "^3.0.0",
    "prettier": "^1.19.1",
    "shelljs": "^0.8.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "standard": {
    "ignore": [
      "/dist/"
    ]
  }
}
